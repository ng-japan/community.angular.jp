{
  "name": "community-angular-jp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "ng run app:build-static",
    "bundlesize": "bundlesize",
    "format": "prettier --write 'src/**/*.{ts,scss,html}'",
    "format:check": "prettier --check 'src/**/*.{ts,scss,html}'",
    "lint": "ng lint && yarn format:check",
    "start": "ng serve app",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "tools:report-bundle-size": "./tools/report-bundle-size.mjs",
    "postinstall": "ngcc"
  },
  "dependencies": {
    "@angular/animations": "12.0.5",
    "@angular/cdk": "12.0.5",
    "@angular/common": "12.0.5",
    "@angular/compiler": "12.0.5",
    "@angular/core": "12.0.5",
    "@angular/forms": "12.0.5",
    "@angular/platform-browser": "12.0.5",
    "@angular/platform-browser-dynamic": "12.0.5",
    "@angular/router": "12.0.5",
    "@ngneat/tailwind": "7.0.3",
    "@nguniversal/common": "12.1.3",
    "comlink": "4.3.1",
    "core-js": "3.19.1",
    "remark": "14.0.1",
    "remark-html": "15.0.0",
    "rxjs": "6.6.7",
    "tslib": "2.3.1",
    "web-animations-js": "2.3.2",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.0.5",
    "@angular-eslint/builder": "12.6.1",
    "@angular-eslint/eslint-plugin": "12.6.1",
    "@angular-eslint/eslint-plugin-template": "12.6.1",
    "@angular-eslint/schematics": "12.6.1",
    "@angular-eslint/template-parser": "12.6.1",
    "@angular/cli": "12.0.5",
    "@angular/compiler-cli": "12.0.5",
    "@angular/language-service": "12.0.5",
    "@nguniversal/builders": "12.1.3",
    "@testing-library/angular": "10.11.0",
    "@testing-library/jest-dom": "5.15.0",
    "@types/jest": "27.0.3",
    "@types/node": "14.18.0",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "autoprefixer": "10.4.0",
    "eslint": "7.32.0",
    "filesize": "8.0.6",
    "globby": "12.0.2",
    "jest": "27.3.1",
    "jest-preset-angular": "10.1.0",
    "postcss": "8.4.5",
    "prettier": "2.4.1",
    "tailwindcss": "2.2.19",
    "ts-jest": "27.0.7",
    "typescript": "4.2.4",
    "zx": "4.2.0"
  },
  "resolutions": {
    "parse-entities": "3.0.0"
  },
  "bundlesize": [
    {
      "path": "./dist/app/*.{js,css}",
      "compression": "none"
    }
  ]
}
